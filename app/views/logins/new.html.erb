<div class="container">
  <div class="row center sign-up-form">
    <div class="col s12 m12 l12">
      <div class="card-panel">
        <h3 class="header">Log in</h3>
        <div class="row">
          <%= form_for(:logins, html: {class: "col s12"}) do |f| %>
            

            <div class="row">
              <div class="input-field col s12">
                <%= f.label :mobile_no %>  
                <%= f.text_field :mobile_no, placeholder: "Enter your mobile number" %>
              </div>
            </div>

            <div class="row">
              <div class="input-field col s12">
                <%= f.label :password %>  
                <%= f.password_field :password, placeholder: "Enter your password" %>
              </div>
            </div>
      
           
            <div class="row">
              <div class="input-field col s12">
                <%= f.button "Log in"%> 
              </div>
            </div>
            
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
